      , "guzzlehttp/psr7": "^2.6"
      , "symfony/http-foundation": "^4.0|^5.0|^6.0|^7.0"
      , "symfony/routing": "^4.0|^5.0|^6.0|^7.0"
    }
  , "require-dev": {
        "phpunit/phpunit": "~9"
    }
